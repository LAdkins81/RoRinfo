
<a href="/songs">Dashboard</a>
<h3>Others who added <%= @song.title%></h3>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Times Added</th>
    </tr>
  </thead>
  <tbody>
    <% @song.users.distinct.each do |person| %>
    <tr>
      <td><a href='/users/<%=person.id%>'><%= person.first_name %> <%= person.last_name %></a></td>
      <% @count.each do |key, value| %>
        <%if person.id == key %>
          <td><%= value %></td>
        <% end %>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>
