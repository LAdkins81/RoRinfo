<a href="/songs">Dashboard</a>

<h1><%= @user.first_name%> <%= @user.last_name %>'s Playlist</h1>

<table>
  <thead>
    <tr>
      <th>Artist</th>
      <th>Title</th>
      <th>Times Added</th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |s| %>
    <tr>
      <td><%= s.artist %></td>
      <td><%= s.title %></td>
      <% @count.each do |key, value| %>
        <%if s.id == key %>
          <td><%= value %></td>
        <% end %>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>
